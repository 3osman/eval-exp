<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane focusTraversable="true" onKeyPressed="#stopTrial" style="-fx-background-color: DF9694;" stylesheets="@bootstrap.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="evalspeedmotion.view.TrialExperimentController">
   <children>
      <Label fx:id="timerLabel" alignment="CENTER" layoutX="10.0" layoutY="100.0" style="-fx-text-fill: WHITE;" styleClass="h4" text="Timer" />
      <StackPane fx:id="wheel" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Circle radius="10.0" stroke="BLACK" strokeType="INSIDE" translateY="50.0" />
         </children>
      </StackPane>
      <Text fill="WHITE" layoutX="10.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h3" text="Participant: " />
      <Text fx:id="participant" fill="WHITE" layoutX="125.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h3" text="#" />
      <HBox layoutX="10.0" layoutY="50.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h3" text="Block: " />
            <Text fx:id="block" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h3" text="0" />
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h3" text=" / " />
            <Text fx:id="totalBlocks" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h3" text="0" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
